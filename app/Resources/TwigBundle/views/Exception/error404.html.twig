                        </li>
                    </ul>
                </div>

                <hr />
                <div class="space"></div>

                <div class="center">
                    <a href="javascript:history.back()" class="btn btn-grey">
                        <i class="ace-icon fa fa-arrow-left"></i>
                        Revenir en arrière
                    </a>

                    <a href="{{ path('nb_main_homepage') }}" class="btn btn-primary">
                        <i class="ace-icon fa fa-tachometer"></i>
                        Accueil
                    </a>
                </div>
            </div>
        </div>

        <!-- PAGE CONTENT ENDS -->
    </div><!-- /.col -->
</div><!-- /.row -->


{% endblock %}


